@model IEnumerable<AboutUs>

<div class="breadcrumb-area">
    <div class="container">
        <ol class="breadcrumb breadcrumb-list">
            <li class="breadcrumb-item"><a asp-action="Index">Anasayfa</a></li>
            <li class="breadcrumb-item active">Hakkımızda </li>
        </ol>
    </div>
</div>
<div class="skill-area white-bg ptb-90">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="about-content mb-all-40">
                    @foreach (var item in Model)
                    {
                        <div class="about-title">
                            <h3>@item.Title</h3>
                        </div>
                        <p>@Html.Raw(item.Text)</p>
                    }
                </div>
            </div>
        </div>
    </div>
</div>

@await Component.InvokeAsync("CustomerReviews")

@await Component.InvokeAsync("OurTeams")